   "source": [
    "function Promedio(a,b) #En esta parte se aplicaran los valores que permitiran el calculo del promedio\n",
    "    c=0 
    "    c=c+(a+b)/2 #El promedio\n puedo obtenerlo con este modelo matemático",
    "    return c #Imprimo c\n, para generar su valor resultante",
    "end"
   ]
