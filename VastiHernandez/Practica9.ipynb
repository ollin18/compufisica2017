    "function SMatriz(A,B) \n",
    "    if size(A)==size(B)==(2,2) \n",
    "        R=zeros(size(A)) 
    "        for i in 1:size(A)[1] ,
    "            for j in 1:size(A)[2] 
    "                c=A[i,j]+B[i,j] 
    "                R[i,j]=c 
    "            end\n",
    "        end\n",
    "        print(R) 
    "    end\n",
    "end"
   ]
