   ],
   "source": [
    "function prodcruz3(A,B)\n",
    "    if length(A)==length(B)==3 
    "        R = 0 
    "        for i in 1:length(A) 
    "            R=R+(A[i]*B[i]) 
    "        end\n",
    "        print(R),
    "    else\n",
    "        println(\"Vectores de distinta dimensión\"),
